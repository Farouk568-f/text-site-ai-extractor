{
  "version": 2,
  "builds": [
    {
      "src": "article_api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "article_api.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_APP": "article_api.py"
  },
  "functions": {
    "article_api.py": {
      "maxDuration": 30
    }
  }
}
